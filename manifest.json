{
  "manifest_version": 2,

  "name": "Add-Art",
  "description": "Replacing ads with art.",
  "author" : "Corey Tegeler",
  "version": "0.1",
  
  "browser_action": {
    "default_icon": "images/icon-64.png",
    "default_popup": "interface.html",
    "default_title": "Add art!"
  },
  "permissions": [
    "storage",
    "http://*.add-art.org/"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.11.2.min.js",
      "js/helpers.js",
      "js/defaultSources.js",
      "js/main.js"
    ],
    "persistent": false
  }
}